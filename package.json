{
  "name": "soda-angular",
  "description": "Socrata SODA client for Angular",
  "version": "0.0.11",
  "author": {
    "name": "Dave Sutherland",
    "email": "dave@daveography.ca",
    "url": "https://github.com/Daveography"
  },
  "homepage": "https://github.com/Daveography/soda-angular",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daveography/soda-angular.git"
  },
  "keywords": [
    "socrata",
    "soda",
    "soql",
    "angular",
    "open data"
  ],
  "scripts": {
    "build": "tsc && cpx package.json dist",
    "test": "ts-node --project spec/tsconfig.spec.json node_modules/jasmine/bin/jasmine",
    "lint": "tslint -p ./",
    "prepare": "npm run build && npm test && npm run lint",
    "prepublishOnly": "echo 'Do not run npm publish directly, use npm run dist' && exit 1",
    "dist": "npm run prepare && cd dist && npm publish --ignore-scripts"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "dependencies": {
    "@angular/common": "^7.1.0",
    "@angular/core": "^7.1.0",
    "npm": "^6.11.3",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@types/jasmine": "^3.4.0",
    "braces": "^3.0.2",
    "cpx": "^1.5.0",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "kind-of": ">=6.0.3",
    "mem": "^4.0.0",
    "minimist": ">=1.2.2",
    "ts-auto-mock": "^1.5.7",
    "ts-node": "^8.8.1",
    "ts-patch": "^1.0.7",
    "tslib": "^1.11.1",
    "tslint": "^6.1.1",
    "typescript": "~3.8.3"
  }
}
